{
  "projectName": "Employee",
  "packageName": "com.liferay.sb.employee",
  "liferayVersion": "7.1",
  "applications": [{
      "model": "Employee",
      "title": "Employee Test",
      "web": "true",
      "asset": {
        "assetTitleFieldName": "employeeTitleName",
        "assetSummaryFieldName": "employeeSummaryName",
